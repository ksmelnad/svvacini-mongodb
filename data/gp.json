{
  "book_title": "गणेशपञ्चरत्नम्",
  "book_id": "BV_SH_SS_PS_GP",
  "audio": "https://d245epgxjv7tw3.cloudfront.net/SG/BV_SH_SS_PS_GP/SC1_1.mp3",
  "content": [
    {
      "order": 1,
      "lines": [
        {
          "text": "मुदाकरात्तमोदकं सदा विमुक्तिसाधकं",
          "begin": "0.000",
          "end": "7.377"
        },
        {
          "text": "कलाधरावतंसकं विलासिलोकरक्षकम् ।",
          "begin": "7.377",
          "end": "14.069"
        },
        {
          "text": "अनायकैकनायकं विनाशितेभदैत्यकं",
          "begin": "14.069",
          "end": "20.703"
        },
        {
          "text": "नताशुभाशुनाशकं नमामि तं विनायकम् ॥१॥",
          "begin": "20.703",
          "end": "28.415"
        }
      ]
    },
    {
      "order": 2,
      "lines": [
        {
          "text": "नतेतरातिभीकरं नवोदितार्कभास्वरं",
          "begin": "28.415",
          "end": "35.158"
        },
        {
          "text": "नमत्सुरारिनिर्जरं नताधिकापदुद्धरम् ।",
          "begin": "35.158",
          "end": "42.339"
        },
        {
          "text": "सुरेश्वरं निधीश्वरं गजेश्वरं गणेश्वरं",
          "begin": "42.339",
          "end": "48.814"
        },
        {
          "text": "महेश्वरं तमाश्रये परात्परं निरन्तरम् ॥२॥",
          "begin": "48.814",
          "end": "55.875"
        }
      ]
    },
    {
      "order": 3,
      "lines": [
        {
          "text": "समस्तलोकशङ्करं निरस्तदैत्यकुञ्जरं",
          "begin": "55.875",
          "end": "62.171"
        },
        {
          "text": "दरेतरोदरं वरं वरेभवक्त्रमक्षरम् ।",
          "begin": "62.171",
          "end": "68.490"
        },
        {
          "text": "कृपाकरं क्षमाकरं मुदाकरं यशस्करं",
          "begin": "68.490",
          "end": "74.397"
        },
        {
          "text": "मनस्करं नमस्कृतां नमस्करोमि भास्वरम् ॥३॥",
          "begin": "74.397",
          "end": "81.088"
        }
      ]
    },
    {
      "order": 4,
      "lines": [
        {
          "text": "अकिञ्चनार्तिमार्जनं चिरन्तनोक्तिभाजनं",
          "begin": "81.088",
          "end": "87.422"
        },
        {
          "text": "पुरारिपूर्वनन्दनं सुरारिगर्वचर्वणम् ।",
          "begin": "87.422",
          "end": "93.899"
        },
        {
          "text": "प्रपञ्चनाशभीषणं धनञ्जयादिभूषणम्",
          "begin": "93.899",
          "end": "100.309"
        },
        {
          "text": "कपोलदानवारणं भजे पुराणवारणम् ॥४॥",
          "begin": "100.309",
          "end": "106.945"
        }
      ]
    },
    {
      "order": 5,
      "lines": [
        {
          "text": "नितान्तकान्तदन्तकान्तिमन्तकान्तकात्मजम्",
          "begin": "106.945",
          "end": "113.651"
        },
        {
          "text": "अचिन्त्यरूपमन्तहीनमन्तरायकृन्तनम् ।",
          "begin": "113.651",
          "end": "119.960"
        },
        {
          "text": "हृदन्तरे निरन्तरं वसन्तमेव योगिनां",
          "begin": "119.960",
          "end": "126.464"
        },
        {
          "text": "तमेकदन्तमेव तं विचिन्तयामि सन्ततम् ॥५॥",
          "begin": "126.464",
          "end": "133.319"
        }
      ]
    },
    {
      "order": 6,
      "lines": [
        {
          "text": "महागणेशपञ्चरत्नमादरेण योऽन्वहं",
          "begin": "133.319",
          "end": "139.360"
        },
        {
          "text": "प्रजल्पति प्रभातके हृदि स्मरन् गणेश्वरम् ।",
          "begin": "139.360",
          "end": "146.458"
        },
        {
          "text": "अरोगतामदोषतां सुसाहितीं सुपुत्रतां",
          "begin": "146.458",
          "end": "153.260"
        },
        {
          "text": "समाहितायुरष्टभूतिमभ्युपैति सोऽचिरात् ॥६॥",
          "begin": "153.260",
          "end": "161.989"
        }
      ]
    }
  ]
}
